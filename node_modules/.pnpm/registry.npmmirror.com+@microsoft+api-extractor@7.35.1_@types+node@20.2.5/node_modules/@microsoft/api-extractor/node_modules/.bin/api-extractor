#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+@microsoft+api-extractor@7.35.1_@types+node@20.2.5/node_modules/@microsoft/api-extractor/bin/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+@microsoft+api-extractor@7.35.1_@types+node@20.2.5/node_modules/@microsoft/api-extractor/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+@microsoft+api-extractor@7.35.1_@types+node@20.2.5/node_modules/@microsoft/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+@microsoft+api-extractor@7.35.1_@types+node@20.2.5/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+@microsoft+api-extractor@7.35.1_@types+node@20.2.5/node_modules/@microsoft/api-extractor/bin/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+@microsoft+api-extractor@7.35.1_@types+node@20.2.5/node_modules/@microsoft/api-extractor/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+@microsoft+api-extractor@7.35.1_@types+node@20.2.5/node_modules/@microsoft/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+@microsoft+api-extractor@7.35.1_@types+node@20.2.5/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/api-extractor" "$@"
else
  exec node  "$basedir/../../bin/api-extractor" "$@"
fi
