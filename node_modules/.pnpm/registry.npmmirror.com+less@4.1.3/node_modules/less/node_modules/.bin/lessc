#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules/less/bin/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules/less/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules/less/bin/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules/less/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+less@4.1.3/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/lessc" "$@"
else
  exec node  "$basedir/../../bin/lessc" "$@"
fi
