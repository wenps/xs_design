#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+browser-sync@2.29.3/node_modules/browser-sync/dist/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+browser-sync@2.29.3/node_modules/browser-sync/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+browser-sync@2.29.3/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+browser-sync@2.29.3/node_modules/browser-sync/dist/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+browser-sync@2.29.3/node_modules/browser-sync/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/registry.npmmirror.com+browser-sync@2.29.3/node_modules:/mnt/c/Users/xiaoe/Desktop/xsDesign/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browser-sync/dist/bin.js" "$@"
else
  exec node  "$basedir/../browser-sync/dist/bin.js" "$@"
fi
